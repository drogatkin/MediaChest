<HTML><HEAD><TITLE>@title@</TITLE>
<meta http-equiv="Content-Type" content="text/html; charset=@charset@">
<style>
H4{margin-bottom:5px;color:lightgrey}
IMG{margin-right:3px;margin-left:10px;margin-top 1px}
LI{font-size:10pt;color:#007000;margin:1px}
OL{margin-top:0px;margin-bottom:0px}
TD{FONT-FAMILY:Tahoma;FONT-SIZE:9pt;}
BODY{margin:0px;BACKGROUND-COLOR:white;COLOR:black;FONT-FAMILY:Tahoma;FONT-SIZE:8pt}
.tdMain1{border:1px solid tan;background:white}
.tdMain2{border:1px solid tan;background:ivory}
.tdStatHdr{font-weight:bold;color:red}
.bHdr{border-bottom:1px solid tan}
.bArtHdr{font-size:10pt;color:#000070;text-decoration: none;cursor:hand}
.bArtHdr:hover{font-size:10pt;color:"red";text-decoration: none;cursor:hand}
.bVAHdr{font-size:10pt;color:#007000;text-decoration: none;cursor:hand}
.bVAHdr:hover{font-size:10pt;color:"red";text-decoration: none;cursor:hand}
.bPlaylistHdr{font-size:10pt;color:#007000;text-decoration: none;cursor:hand}
.bPlaylistHdr:hover{font-size:10pt;color:"red";text-decoration: none;cursor:hand}
.bAlbumHdr{font-size:10pt;text-decoration: none;color:#007000;cursor:hand;position:relative;left:+15}
.bAlbumHdr:hover{font-size:10pt;text-decoration: none;color:"red";cursor:hand}
.subImage{position:relative; left:+15px}
</style>

<SCRIPT LANGUAGE="JavaScript">
var plusImg = new Image();
	plusImg.src = "@imagepath@plusbl.gif"
var minusImg = new Image();
	minusImg.src = "@imagepath@minusbl.gif"

function showAlbums() {
	if (document.getElementsByTagName)
		x = document.getElementsByTagName('div');
	else if (document.all)
		x = document.all.tags('div');
	for (var i=0;i<x.length;i++)
	{
		if (x[i].id.indexOf("Set") != -1) {
			x[i].style.display = "";
		}
	}
	if (document.getElementsByTagName)
		x = document.getElementsByTagName('img');
	else if (document.all)
		x = document.all.tags('img');
	for (var i=0;i<x.length;i++)
	{
		if (x[i].id.indexOf("Set") != -1) {
			x[i].src = minusImg.src;
		}
	}
}

function hideAlbums() {
	if (document.getElementsByTagName)
		x = document.getElementsByTagName('div');
	else if (document.all)
		x = document.all.tags('div');
	for (var i=0;i<x.length;i++)
	{
		if ((x[i].id.indexOf("Set") != -1) || (x[i].id.indexOf("Album") != -1)) {
			x[i].style.display = "none";
		}
	}
	if (document.getElementsByTagName)
		x = document.getElementsByTagName('img');
	else if (document.all)
		x = document.all.tags('img');
	for (var i=0;i<x.length;i++)
	{
		if ((x[i].id.indexOf("Set") != -1) || (x[i].id.indexOf("Album") != -1)) {
			x[i].src = plusImg.src;
		}
	}
}

function showLevel( _levelId, _imgId ) {
	var thisLevel = document.getElementById( _levelId );
	var thisImg = document.getElementById( _imgId );
	if ( thisLevel.style.display == "none") {
		thisLevel.style.display = "";
		thisImg.src = minusImg.src;
		}
	else {
		thisLevel.style.display = "none";
		thisImg.src = plusImg.src;
		}
}
</SCRIPT>
</HEAD><BODY>
<!-- $Id: ipod-html.htmp,v 1.4 2005/01/21 05:05:29 rogatkin Exp $   
    All credits for this template go to the author of tunes2html -->
<div id=dvMain align=center>
<table style="border-bottom:1px solid tan" cellspacing=0 cellpadding=5 width="100%">
<tr bgcolor=white><td valign=middle><h2 style="margin-bottom:5px">@user@'s iPod Contents</h2></td><td align=right valign=middle>
<h4 style="margin-bottom:5px;color:lightgrey">@date@</h4></td></tr>
</table>
<form action="none">
<table cellspacing=5 cellpadding=5 width="100%">
<tr><td valign=top class=tdMain2>
<b>This page generated by <a href=http://mediachest.sourceforge.net>MediaChest</a></b><p>
<b class=bHdr>Statistics</b><p>
<div id=dvStat style="margin-left:20px">
<table cellspacing=0 cellpadding=2>
<tr><td class=tdStatHdr>Sets:</td><td>@sets@</td></tr>
<tr><td class=tdStatHdr>Discs:</td><td>@disks@</td></tr>
<tr><td class=tdStatHdr>Tracks:</td><td>@tracks@</td></tr>
<tr><td class=tdStatHdr>Playing Time:</td><td>@playtime@</td></tr>
<tr><td class=tdStatHdr>Space Used:</td><td>@size@</td></tr>
</table>
</div>
</td></tr>
<tr>
  <td valign=top class=tdMain1>
    <a class=bArtHdr href="javascript:showAlbums();"><b>Show All Albums</b></a><br>
    <a class=bArtHdr href="javascript:hideAlbums();"><b>Hide All Albums</b></a></p>
    <b class=bHdr>Single Artists</b></p>
    @artists(
    <a class=bArtHdr href="javascript:showLevel('Set@sets@','imgSet@sets@');">
    <img id=imgSet@sets@ border=0 src=plusbl.gif><b>@title@ (D:@disks@, T:@tracks@) [@playtime@]</b></a><br>
    <div id=Set@sets@ style='display:none'>
    @albums(
    <a href="javascript:showLevel('Album@disks@','imgAlbum@disks@');">
     <img border=0 id=imgAlbum@disks@ class=subImage src=plusbl.gif></a>
     <a href="javascript:showLevel('Album@disks@','imgAlbum@disks@');" class=bAlbumHdr>
      <b>@title@ - @year@ (T:@tracks@) [@playtime@]</b></a><br>
        <div id=Album@disks@ style='position:relative;left:+15px;display:none'>
          <ol>@songs(<li> @title@ [@playtime@]<br><!--input type="text" name="@title@"-->
          )@
          </ol>
       </div>
    )@
    </div>
    )@
  </td>
</tr>
</table>
</form>
</body></html>
